{
  "name": "typescript-react-pomodoro",
  "version": "0.1.0",
  "dependencies": {
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.24",
    "@types/react": "^16.0.4",
    "@types/react-dom": "^15.5.4",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-github-corner": "^1.0.0",
    "react-scripts-ts": "2.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuoXiaoyang/typescript-react-pomodoro.git"
  },
  "keywords": [
    "Pomodoro"
  ],
  "author": "guoxiaoyang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GuoXiaoyang/typescript-react-pomodoro/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "enzyme": "^2.9.1",
    "react-addons-test-utils": "^15.6.0"
  }
}
